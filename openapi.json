{
    "openapi": "3.0.3",
    "info": {
      "title": "Cars Binar Rent Documentation",
      "description": "Dokumentasi API Rental obil",
      "contact": {
        "email": "nexsel@gmail.com"
      },
      "version": "1.0"
    },
    "servers": [
      {
        "url": "http://localhost:5000/api/v1"
      },
      {
        "url": "http://localhost:3000/api/v1"
      }
    ],
    "tags": [
      {
        "name": "auth",
        "description": "Everything about authentication"
      },
      {
        "name": "cars",
        "description": "Cars CRUD"
      },
      {
        "name": "users",
        "description": "Users CRUD"
      }
    ],
    "paths": {
      "/login": {
        "post": {
          "tags": [
            "auth"
          ],
          "summary": "Do login",
          "description": "login",
          "operationId": "login",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string",
                      "example": "test12@gmail.com"
                    },
                    "password": {
                      "type": "string",
                      "example": "test123"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Successful Operation",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Success"
                      },
                      "message": {
                        "type": "string",
                        "example": "Berhasil Login!"
                      },
                      "data": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "example": 2
                          },
                          "name": {
                            "type": "string",
                            "example": "Hello World!"
                          },
                          "email": {
                            "type": "string",
                            "example": "user@mail.com"
                          },
                          "token": {
                            "type": "string",
                            "example": "13810a0sedu1231.129309a7sd071923.1293u0as7d0as70213"
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "401": {
              "description": "Login Failed!",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Failed"
                      },
                      "message": {
                        "type": "string",
                        "example": "Gagal login password salah!"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/register": {
        "post": {
          "tags": [
            "auth"
          ],
          "summary": "register user data",
          "description": "register",
          "operationId": "register",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string",
                      "example": "test@mail.com"
                    },
                    "password": {
                      "type": "string",
                      "example": "test123"
                    },
                    "nama": {
                      "type": "string",
                      "example": "Teest"
                    },
                    "avatar": {
                      "type": "string",
                      "example": "Gambar"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Success"
                      },
                      "message": {
                        "type": "string",
                        "example": "Berhasil Register!"
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Email sudah terdaftar",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Fail!"
                      },
                      "message": {
                        "type": "string",
                        "example": "Email sudah terdaftar!"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/cars": {
        "get": {
          "tags": [
            "cars"
          ],
          "summary": "Get books lists",
          "description": "Get books Lists",
          "operationId": "getBooks",
          "responses": {
            "200": {
              "description": "Successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "title": {
                          "type": "string",
                          "example": "Buku A"
                        },
                        "author": {
                          "type": "string",
                          "example": "Shakespear"
                        },
                        "price": {
                          "type": "number",
                          "example": 50000
                        },
                        "cover": {
                          "type": "string",
                          "example": "http://cloudinary.com/213125123"
                        }
                      }
                    }
                  }
                }
              }
            },
            "500": {
              "description": "Internal Server Error",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Error!"
                      },
                      "code": {
                        "type": "integer",
                        "example": 500
                      },
                      "message": {
                        "type": "string",
                        "example": "Internal Server Error!"
                      }
                    }
                  }
                }
              }
            }
          },
          "security": [
            {
              "auth": []
            }
          ]
        },
        "post": {
          "tags": [
            "cars"
          ],
          "summary": "Add a new books",
          "description": "Add a new books",
          "operationId": "addBooks",
          "requestBody": {
            "required": true,
            "content": {
              "multipart/form-data": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string",
                      "example": "Buku A"
                    },
                    "author": {
                      "type": "string",
                      "example": "Shakespear"
                    },
                    "price": {
                      "type": "number",
                      "example": 50000
                    },
                    "cover": {
                      "type": "string",
                      "format": "binary",
                      "example": "http://www.africau.edu/images/default/sample.pdf"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Success"
                      },
                      "message": {
                        "type": "string",
                        "example": "Berhasil Register!"
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Email sudah terdaftar",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Fail!"
                      },
                      "message": {
                        "type": "string",
                        "example": "Email sudah terdaftar!"
                      }
                    }
                  }
                }
              }
            }
          },
          "security": [
            {
              "auth": []
            }
          ]
        }
      }
    },
    "components": {
      "securitySchemes": {
        "auth": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT"
        }
      }
    }
  }